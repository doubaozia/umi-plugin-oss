{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "interface-name": [
      true,
      "always-prefix",
      "never-prefix"
    ],
    "space-before-function-paren": [
      "error",
      "always"
    ],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
